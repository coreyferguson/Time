{
  "name": "time-api",
  "version": "1.0.0",
  "description": "Free time tracker. Use for tracking study time, work project time, or anything else you want.",
  "private": true,
  "scripts": {
    "debug": "stage=test ./node_modules/.bin/mocha --recursive --watch --inspect --debug-brk test/spec",
    "deploy": "node_modules/.bin/sls deploy --stage=prod",
    "test": "stage=test ./node_modules/.bin/mocha-webpack --webpack-config=test/webpack.config.js test/**/*.spec.js",
    "watch": "stage=test ./node_modules/.bin/mocha-webpack --webpack-config=test/webpack.config.js --watch test/**/*.spec.js"
  },
  "dependencies": {
    "firebase-admin": "5.12.1",
    "google-auth-library": "1.4.0",
    "jsonwebtoken": "^8.3.0",
    "oauth2-server": "3.0.0",
    "promise-filter-chain": "1.0.2",
    "serverless-secrets": "3.0.0-beta.12",
    "uuid": "3.2.1"
  },
  "devDependencies": {
    "aws-sdk": "2.230.1",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "chai-string": "1.4.0",
    "dev-env-lib": "1.1.3",
    "json-loader": "^0.5.7",
    "local-dynamo-facade": "1.0.1",
    "mocha": "5.1.1",
    "mocha-webpack": "^2.0.0-beta.0",
    "os": "0.1.1",
    "serverless": "1.27.3",
    "serverless-webpack": "^5.1.5",
    "sinon": "4.5.0",
    "sinon-chai": "3.0.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.3",
    "webpack-node-externals": "^1.7.2",
    "yaml-loader": "^0.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coreyferguson/time-api.git"
  },
  "keywords": [
    "time",
    "tracker",
    "track",
    "ifttt",
    "api",
    "node",
    "javascript"
  ],
  "author": "Corey Ferguson <corey.t.ferguson@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coreyferguson/time-api/issues"
  },
  "homepage": "https://github.com/coreyferguson/time-api#readme"
}
