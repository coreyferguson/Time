{
  "name": "time-api",
  "version": "1.0.0",
  "description": "Free time tracker. Use for tracking study time, work project time, or anything else you want.",
  "private": true,
  "scripts": {
    "start": "node test/local/graphql",
    "debug": "stage=test ./node_modules/.bin/mocha --recursive --watch --inspect --debug-brk test/spec",
    "deploy": "node_modules/.bin/sls deploy",
    "test": "stage=test ./node_modules/.bin/mocha --recursive test/spec/**/*.js",
    "watch": "stage=test ./node_modules/.bin/mocha --recursive --watch  test/spec/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coreyferguson/time-api.git"
  },
  "keywords": [
    "time",
    "tracker",
    "track",
    "ifttt",
    "api",
    "node",
    "javascript"
  ],
  "author": "Corey Ferguson <corey.t.ferguson@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/coreyferguson/time-api/issues"
  },
  "homepage": "https://github.com/coreyferguson/time-api#readme",
  "dependencies": {
    "apollo-datasource": "^0.3.1",
    "apollo-server-lambda": "^2.4.8",
    "bluebird": "^3.5.3",
    "graphql": "^14.1.1",
    "js-base64": "^2.5.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "apollo-server": "^2.4.8",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "local-dynamo-facade": "^1.0.1",
    "mocha": "^6.0.2",
    "serverless": "^1.38.0",
    "serverless-secrets": "^3.0.0-beta.12",
    "sinon": "^7.2.7",
    "sinon-chai": "^3.3.0"
  }
}
